<div class='blog content'>
  <ul class="blog-list">
    {% for page in collections.posts.pages %}
      <li>
        <div style='display: inline-block'>
          <a href="/{{ page.permalink }}">{{ page.title }}</a>
        </div>
        {% comment %} <span>({{ page.published_date | date: "%b %d, %Y" }})</span> {% endcomment %}
      </li>
    {% endfor %}
  </ul>
</div>
